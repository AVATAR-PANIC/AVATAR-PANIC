<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TricorderGoogleMapsViewer"
    >
    
    <fragment 
          android:id="@+id/tricorder_googlemap"
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:name="com.google.android.gms.maps.MapFragment" />
    
    <ImageView
        android:id="@+id/pointkey"
        android:layout_width="75dp"
        android:layout_height="100dp"
        android:background="@drawable/pointkey"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="70dp"
        />
    
     <LinearLayout
            android:id="@+id/logoBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="60dp" >

            <ImageView
                android:id="@+id/airForceWings"
                android:layout_width="50dp"
                android:layout_height="35dp"
                android:background="@drawable/usaf_logo" />

            <ImageView
                android:id="@+id/tecedgeLogoText"
                android:layout_width="150dp"
                android:layout_height="35dp"
                android:background="@drawable/tecedgetransparent" />

            <ImageView
                android:id="@+id/discoveryLabText"
                android:layout_width="100dp"
                android:layout_height="35dp"
                android:background="@drawable/discoverylablogotransparent" />

            <ImageView
                android:id="@+id/afrl_logo"
                android:layout_width="50dp"
                android:layout_height="35dp"
                android:background="@drawable/afrl_logo_transparent" />
        </LinearLayout>
        
     <LinearLayout
         android:id="@+id/tri_map_settings"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentRight="true"
         android:layout_alignParentBottom="true"
         android:layout_marginRight="60dp" >
         
         <ImageView
             android:id="@+id/tri_menu_settings"
             android:layout_width="50dp"
             android:layout_height="50dp"
             android:background="@drawable/menusettings"
             android:onClick="tricorderOnClick" />
         
         <ImageView
             android:id="@+id/tri_menu_upload"
             android:layout_width="50dp"
             android:layout_height="50dp"
             android:background="@drawable/menuupload"
             android:onClick="tricorderOnClick" />
         
         <ImageView
             android:id="@+id/tri_menu_refresh"
             android:layout_width="50dp"
             android:layout_height="50dp"
             android:background="@drawable/menurefresh"
             android:onClick="tricorderOnClick" />
         
     </LinearLayout>
        
</RelativeLayout>